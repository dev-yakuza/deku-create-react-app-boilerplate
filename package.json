{
  "name": "deku-create-react-app-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:ci": "react-scripts test --watchAll=false",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.(css|tsx)'",
    "lint:md": "remark . --quiet --frail",
    "check:spell": "cspell '**' --gitignore --no-progress",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.15",
    "@tanstack/react-query": "^4.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.7",
    "typescript": "^4.9.5",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@emotion/jest": "^11.10.5",
    "@storybook/addon-actions": "^7.0.17",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.59.0",
    "axios-mock-adapter": "^1.21.4",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cspell": "^6.31.1",
    "eslint": "^8.24.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-storybook": "^0.6.11",
    "history": "^5.3.0",
    "postcss": "^8.4.18",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "sb": "^6.5.12",
    "storybook-addon-react-router-v6": "^1.0.1",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^29.0.0",
    "webpack": "^5.76.3"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  },
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ]
  }
}
